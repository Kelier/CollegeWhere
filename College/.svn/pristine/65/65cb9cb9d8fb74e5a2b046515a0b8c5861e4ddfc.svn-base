/* CSS Document */

/*banner开始*/
.banner{height:340px;margin-top:10px;overflow:hidden;position:relative;}
.banner .d1{width:1600px;margin:0 auto;height:340px;display:block;left:0px;top:0px;background-size: 100% 100%;}
.banner .d2{width:100%;height:30px;clear:both;position:absolute;z-index:100;left:0px;bottom:0;}
.banner .d2 ul{float:left;position:absolute;left:50%;margin-left:-32px;top:0;display:inline;}
.banner .d2 li{width:17px;height:15px;overflow:hidden;cursor:pointer;background:url(../images/img1.png) no-repeat center;float:left;margin:0 3px;display:inline;}
.banner .d2 li.nuw{background:url(../images/img1_1.png) no-repeat center;}
/*banner结束*/

/*二级导航开始*/
/*一键导航*/
.one-key-navigation{width:1100px;margin:10px auto;height:122px;padding:0 2px;}
.one-key-navigation a{
	width: 120px;
	height: 100px;
	float: left;
	border: 2px solid #ec8400;
	padding-top:20px;
	margin-left:-2px;
}
.one-key-navigation a:hover{
	background-color:#ec8400;
	color:#FFF;
}
.one-key-navigation a .icon{
	display: block;
	margin:0 36px;
	height: 46px;
	width:46px;
	background-image: url(../images/icon.png);
}
.one-key-navigation a.icon:hover{
	background-image: url(../images/icon.png) ;
}
.one-key-navigation  a .icon1{
	background-position:0 0;
}
.one-key-navigation a  .hover1{
	background-position:0 -66px;
}
.one-key-navigation  a .icon2{
	background-position:-57px 0;
}
.one-key-navigation a .hover2{
	background-position:-57px -66px;
}
.one-key-navigation  a .icon3{
	background-position:-112px 0;
}
.one-key-navigation a .hover3{
	background-position:-112px -66px;
}
.one-key-navigation  a .icon4{
	background-position:-168px 0;
}
.one-key-navigation a .hover4{
	background-position:-168px -66px;
}
.one-key-navigation  a .icon5{
	background-position:-224px 0;
}
.one-key-navigation a .hover5{
	background-position:-224px -66px;
}
.one-key-navigation  a .icon6{
	background-position:-280px 0;
}
.one-key-navigation a .hover6{
	background-position:-280px -66px;
}
.one-key-navigation  a .icon7{
	background-position:-336px 0;
}
.one-key-navigation a .hover7{
	background-position:-336px -66px;
}
.one-key-navigation  a .icon8{
	background-position:-392px 0;
}
.one-key-navigation a .hover8{
	background-position:-392px -66px;
}
.one-key-navigation  a .icon9{
	background-position:-448px 0;
}
.one-key-navigation a .hover9{
	background-position:-448px -66px;
}

/*二级导航结束*/

