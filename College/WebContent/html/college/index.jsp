			      layer.open({			        type: 1			        ,title: false //不显示标题栏			        ,closeBtn: true			        ,area: '450px;'			        ,shade: 0.8			        ,id: 'LAY_layuipro' //设定一个id，防止重复弹出			        ,moveType: 1 //拖拽模式，0或者1			        ,content: login_form			        ,success: function(layero){			           // var btn = layero.find('.layui-layer-btn');			            //换验证码						$("#vcode").click(function(){							var path = "vcode.jsp?id=" + Math.random();							$(this).attr('src', path);						});			         }			      });			    },			    register: function(){				      //示范一个公告层				      layer.open({				        type: 1				        ,title: false //不显示标题栏				        ,closeBtn: true				        ,area: '450px;'				        ,shade: 0.8				        ,id: 'LAY_layuipro' //设定一个id，防止重复弹出				        ,moveType: 1 //拖拽模式，0或者1				        ,content: register_form				        ,success: function(layero){				           // var btn = layero.find('.layui-layer-btn');				            //换验证码							$("#vcode").click(function(){								var path = "vcode.jsp?id=" + Math.random();								$(this).attr('src', path);							});				         }				      });				},			    loginOut: function(){			    	$.ajax({						type : "post",						url : "../../LoginOut",						dataType : "json",						success : function(ret) {							if(ret.loginOutResult){								top.location.reload();							}						},						error : function(ret) {							alert(JSON.stringify(ret));						}					});				}			  };			  $('#LAY_demo .nav-btn').on('click', function(){			    var othis = $(this), method = othis.data('method');			    active[method] ? active[method].call(this, othis) : '';			  });			  //自定义验证规则			  form.verify({			    title: function(value){			      if(value.length < 5){			        return '标题至少得5个字符啊';			      }			    }			    ,pass: [/(.+){6,12}$/, '密码必须6到12位']			    ,vcode: [/(.+){4}$/, '验证码必须4位']			  });			//监听提交			  form.on('submit(login)', function(data){				  layer.msg('登录中...', {					  icon: 16,					  shade: [0.5, '#f5f5f5'],					  scrollbar: false,					  time:1000,					  end: function(){					  $.ajax({							type : "post",							url : "../../Login",							dataType : "json",							data :data.field,							success : function(ret) {								if(ret.state==400){									$('.login-tip').html('验证码错误!');								}else if(ret.loginResult){								//	var index = layer.index; //获取当前弹层的索引号								//	layer.close(index); //关闭当前弹层							       layer.closeAll(); 							       top.location.reload();								}else{									$('.login-tip').html('用户名或密码错误!');								}							},							error : function(ret) {								alert(JSON.stringify(ret));							}						});				      }				  }); 			    return false;			  });			  form.on('submit(register)', function(data){				  				  layer.msg('注册中...', {					  icon: 16,					  shade: [0.5, '#f5f5f5'],					  scrollbar: false,					  time:1000,					  end: function(){						  $.ajax({								type : "post",								url : "../../Register",								dataType : "json",								data :data.field,								success : function(ret) {									//alert(JSON.stringify(ret));									if(ret.state==400){										$('.login-tip').html('验证码错误!');									}else if(ret.state==600){										layer.msg('两次密码不一致', {											  icon: 5,											  shade: [0.5, '#f5f5f5'],											  scrollbar: false,											  time:500										});										$('.login-tip').html('两次密码不一致');									}else if(ret.registerResult>0){ 										 switch(ret.registerResult){											 case 200:												 layer.msg('注册成功，请登录!', {												  icon: 6,												  shade: [0.5, '#f5f5f5'],												  scrollbar: false,												  time:500,												  end: function(){													  layer.closeAll();												  }											 	});												 break;											 case 500:												 layer.msg('用户名已存在!', {													  icon: 5,													  shade: [0.5, '#f5f5f5'],													  scrollbar: false,													  time:500												 	});												 $('.login-tip').html('用户名已存在');												 break;											 case 400:												 layer.msg('注册失败!', {													  icon: 5,													  shade: [0.5, '#f5f5f5'],													  scrollbar: false,													  time:500												 	});												 $('.login-tip').html('注册失败');												 break;										 }									}else{										$('.login-tip').html('用户名或密码错误!');									}								},								error : function(ret) {									layer.closeAll(); 									layer.msg('登录失败，网络异常', {										  icon: 2,										  shade: [0.5, '#f5f5f5'],										  scrollbar: false,										  time:1000,										  end: function(){											 									      }									  }); 								}							});				      }				  });				 			    return false;			  });			});				</script></body></html>